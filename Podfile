platform :ios, '10.3'

 target 'JudoKitObjC' do
   # Comment the next line if you don't want to use dynamic frameworks
   use_frameworks!

   # Pods for JudoKitObjC
   pod 'TrustKit'
   pod 'PayCardsRecognizer'

   target 'JudoKitObjCTests' do
     inherit! :search_paths
     # Pods for testing
     pod 'Mockingjay'

   end

 end

 target 'JudoKitObjCExampleApp' do
   # Comment the next line if you don't want to use dynamic frameworks
   use_frameworks!

   # Pods for JudoKitObjCExampleApp
   pod 'InAppSettingsKit'
   pod 'CocoaDebug'

   target 'JudoKitObjCExampleAppUITests' do
      inherit! :search_paths
      # Pods for testing
    end

 end

 target 'JudoKitSwiftExampleApp' do
   # Comment the next line if you don't want to use dynamic frameworks
   use_frameworks!

   # Pods for JudoKitSwiftExampleApp

 end
